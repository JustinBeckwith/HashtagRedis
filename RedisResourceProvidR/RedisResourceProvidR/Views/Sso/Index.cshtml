@{
    ViewBag.Title = "ResourceProvidR Home";
}

<header>
    <div class="content-wrapper">
        <div class="float-left">
            <p class="site-title">
                <a href="~/">Redis ResourceProvidR</a></p>
        </div>
    </div>
</header>
<div id="body">
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>Redis ResourceProvidR management</h1>
            </hgroup>
        </div>
    </section>
    <section class="content-wrapper main-content clear-fix">
        <h1>@ViewBag.ResourceEntity.Name</h1>
        <div>
            <table>
                <tr><td>Subscription ID:</td><td>@ViewBag.ResourceEntity.SubscriptionId</td></tr>
                <tr><td>Cloud Service Name:</td><td>@ViewBag.ResourceEntity.CloudServiceName</td></tr>
                <tr><td>Cloud Service Region:</td><td>@ViewBag.ResourceEntity.CloudServiceGeoRegion</td></tr>
                <tr><td>ETag:</td><td>@ViewBag.ResourceEntity.ResourceETag</td></tr>
                <tr><td>Plan:</td><td>@ViewBag.ResourceEntity.Plan</td></tr>
                <tr><td>Promo Code:</td><td>@ViewBag.ResourceEntity.PromotionCode</td></tr>
                <tr><td>Resource Type:</td><td>@ViewBag.ResourceEntity.Type</td></tr>
                <tr><td>Last Operation:</td><td>@ViewBag.ResourceEntity.LastOperation</td></tr>
            </table>
        </div>
    </section>
</div>
